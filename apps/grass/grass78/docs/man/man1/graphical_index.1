.TH graphical_index 1 "" "GRASS 7.8.5" "GRASS GIS User's Manual"
\&.img\-list {
list\-style\-type: none;
margin: 0;
padding: 0;
text\-align: center;
}
\&.img\-list li {
display: inline\-block;
position: relative;
width: 8em;
margin: 0;
padding: 0.5em;
margin\-bottom: 1em;
}
\&.img\-list li:hover {
background\-color: #eee;
}
\&.img\-list li img {
float: left;
max\-width: 100%;
background: white;
}
\&.img\-list li span {
text\-align: center;
}
\&.img\-list li a {
color: initial;
text\-decoration: none;
}
\&.img\-list li .name {
margin: 0.1em;
display: block;
color: #409940;
font\-weight: bold;
font\-style: normal;
font\-size: 120%;
}
